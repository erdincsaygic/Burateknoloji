<!-- Contact Form In start -->
<section class="contact-form">
    <div class="overlay pb-120">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-10">
                    <div class="section-header text-center">
                        <h2 class="title">STILPAY İLETİŞİM</h2>
                        <p>Eğer yardıma ihtiyacınız varsa ya da bir sorunuz varsa, sizin için buradayız!</p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-10">
                    <div class="form-area">
                        <form action="#">
                            <div class="row">
                                <div class="col-6">
                                    <div class="single-input d-flex align-items-center">
                                        <input type="text" placeholder="Ad-Soyad" />
                                        <img src="~/ui/assets/img/contact-user-icon.png"
                                             alt="image" />
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="single-input d-flex align-items-center">
                                        <input type="email" placeholder="Email" />
                                        <img src="~/ui/assets/img/contact-email-icon.png"
                                             alt="image" />
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="single-input d-flex align-items-center">
                                        <input type="text" placeholder="Telefon Numarası" />
                                        <img src="~/ui/assets/img/contact-phone-icon.png"
                                             alt="image" />
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="single-input d-flex align-items-center">
                                        <input type="text" placeholder="Konu" />
                                        <img src="~/ui/assets/img/contact-com-icon.png"
                                             alt="image" />
                                    </div>
                                </div>

                            </div>
                            <div class="single-input d-flex align-items-center">
                                <input type="text" placeholder="Size Nasıl Yardımcı Olabiliriz ?">
                            </div>
                            <div class="single-input d-flex align-items-center">
                                <textarea placeholder="Mesaj"
                                          cols="30"
                                          rows="10"></textarea>
                            </div>
                            <div class="btn-area text-center">
                                <button class="cmn-btn">Gönder</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Contact Form In end -->
